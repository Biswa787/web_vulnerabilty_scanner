{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ],
    "env": {
      "NODE_ENV": "production",
      //"API_URL": "http://localhost" // Replace with your actual API URL
    },
    "functions": {
      "api/**/*.ts": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }